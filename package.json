{
  "name": "magnet.me-example-project",
  "version": "0.0.0",
  "description": "",
  "author": "Tiddo Langerak",
  "devDependencies": {
    "browserify": "^5.11.2",
    "browserify-shim": "^3.7.0",
    "connect": "^3.2.0",
    "gulp": "^3.8.8",
    "gulp-less": "^1.3.5",
    "gulp-watch": "^1.0.3",
    "serve-static": "^1.6.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ],
    "paths": [
      "./src/",
      "./bower_components/"
    ]
  },
  "browser" : {
    "angular" : "./bower_components/angular/angular.js",
    "ui-router": "./bower_components/angular-ui-router/release/angular-ui-router.js",
    "lodash" : "./bower_components/lodash/dist/lodash.js"
  },
  "browserify-shim" : {
    "angular" : "angular",
    "ui-router" : {
      "depends" : ["angular"],
      "exports" : null
    },
    "ui-router.stateHelper" : {
      "dependes" : ["ui-router", "angular"],
      "exports" : null
    }
  }

}
